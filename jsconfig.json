{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      },
      "target": "ES6",
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"],
    "noEmit": true,
    "vueCompilerOptions": {
      "experimentalDisableTemplateSupport": true
    }
}
  